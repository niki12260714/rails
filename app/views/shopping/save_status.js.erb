<% if @errors.nil? and @orignal_error_message.nil? %>
  $("#shopping-modal").modal('hide');
  $('.modal-backdrop').hide();
  <% if @rtn == "hs" %>
    $("#history-result").html("<%= escape_javascript(render 'end_list') %>")
  <% else %>
    location.reload();
  <% end %>
<% else %>
  $("#errors_area_status").html("<%= escape_javascript(render 'layouts/error') %>")
<% end %>